<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="container" prefHeight="430.0" prefWidth="673.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.standardeleven.project.gui.controller.Controller_CreateActivity">
   <children>
      <Label layoutX="217.0" layoutY="35.0" text="Nueva actividad">
         <font>
            <Font name="Consolas Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="47.0" layoutY="81.0" text="Título">
         <font>
            <Font name="Consolas Italic" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="txtTitle" layoutX="133.0" layoutY="77.0" prefHeight="26.0" prefWidth="200.0" />
      <Label layoutX="28.0" layoutY="126.0" text="Descripción">
         <font>
            <Font name="Consolas Italic" size="14.0" />
         </font>
      </Label>
      <TextArea fx:id="txtDescription" layoutX="133.0" layoutY="118.0" prefHeight="200.0" prefWidth="200.0" />
      <Label layoutX="447.0" layoutY="81.0" text="Fecha de entrega">
         <font>
            <Font name="Consolas Italic" size="14.0" />
         </font>
      </Label>
      <DatePicker fx:id="txtDeliveryDate" layoutX="422.0" layoutY="113.0" onAction="#getDeliveryDate" />
      <Button fx:id="buttonPublish" layoutX="450.0" layoutY="373.0" mnemonicParsing="false" onAction="#publishActivity" text="Publicar">
         <font>
            <Font name="Consolas" size="13.0" />
         </font>
      </Button>
      <Button fx:id="buttonCancel" layoutX="559.0" layoutY="373.0" mnemonicParsing="false" onAction="#returnHome" text="Cancelar">
         <font>
            <Font name="Consolas" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
